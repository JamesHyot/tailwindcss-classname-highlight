{
  "publisher": "hyoban",
  "name": "tailwindcss-classname-highlight",
  "displayName": "Tailwind CSS ClassName Highlight",
  "version": "0.1.3",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "description": "",
  "author": "Stephen Zhou <hi@hyoban.cc>",
  "license": "MIT",
  "homepage": "https://github.com/hyoban/tailwindcss-classname-highlight#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyoban/tailwindcss-classname-highlight"
  },
  "bugs": {
    "url": "https://github.com/hyoban/tailwindcss-classname-highlight/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [
    "workspaceContains:**/tailwind.config.ts",
    "workspaceContains:**/tailwind.config.js",
    "workspaceContains:**/tailwind.config.cjs",
    "workspaceContains:**/tailwind.config.mjs"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@antfu/ni": "^0.21.12",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^20.11.17",
    "@types/vscode": "1.84.0",
    "@vscode/vsce": "^2.23.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "micromatch": "^4.0.5",
    "pnpm": "^8.15.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vitest": "^1.2.2"
  }
}
